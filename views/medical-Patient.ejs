<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <h1>Medical Record <%= nameTmp %></h1> 
    <a href="/logout"><button type="button">logout</button></a> 

    <% const {email, role, MedicalRecordDoctor}=record %>
        <table border="1">
            <th >Diagnosis</th>
            <th>Treatment</th>
            <th>Biaya</th>
            <% MedicalRecordDoctor.forEach(el=>{ %>
                <tr>
                    <td>
                        <%= el.diagnosis %>
                    </td>
                    <td>
                        <%= el.treatment %>
                    </td>
                    <td>
                        <%= el.cost %>
                    </td>
                    
                </tr>
                <% }) %>

        </table>
        
</body>

</html>